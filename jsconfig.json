{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "src",
    "paths": {
      "@/*": ["/*"],
      "@apis/*": ["apis/*"],
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@hooks/*": ["hooks/*"],
      "@pages/*": ["pages/*"],
      "@contexts/*": ["contexts/*"],
      "@router/*": ["router/*"],
      "@stores/*": ["stores/*"],
      "@utils/*": ["utils/*"],
      "@styles/*": ["styles/*"]
    }
  },
  "include": [
    "src",
    "custom.d.ts",
    "**/*.js",
    "**/*.jsx",
    "tsconfig.json",
    "**/*.ts",
    "**/*.tsx"
  ]
}
