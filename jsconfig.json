{
  "compilerOptions": {
    "jsx": "react",
    "baseUrl": "src",
    "paths": {
      "@*": ["*"],
      "@apis/*": ["apis/*"],
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@hooks/*": ["hooks/*"],
      "@pages/*": ["pages/*"],
      "@contexts/*": ["contexts/*"],
      "@queries/*": ["queries/*"],
      "@router/*": ["router/*"],
      "@stores/*": ["stores/*"],
      "@utils/*": ["utils/*"],
      "@styles/*": ["styles/*"]
    }
  },
  "include": ["src", "**/*.js", "**/*.jsx", "vite.config.cjs"]
}
